{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "functions": {
    "app.py": {
      "maxDuration": 900
    }
  },
  "env": {
    "PYTHONUNBUFFERED": "1",
    "VERCEL": "1"
  }
} 